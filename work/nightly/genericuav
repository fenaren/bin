#!/bin/tcsh

# Reset this view back to default; gets rid of all view-private files, among
# other things
view-default

# Set build options
cd /vobs/aircraft++
build-default
build-models genericuav
build-plugins serialportparser matlab2010a
build-buildoption PRODUCTION

# Actually run the build
kw-nightly GenericUav "clearmake -B ~/.bldhost.jedi.ndc.nasa.gov -V sim"
kw-sync GenericUav LaSRS_Core
